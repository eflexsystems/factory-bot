{
  "name": "@eflexsystems/factory-bot",
  "description": "A factory library for Node.js and JavaScript inspired by factory_girl",
  "exports": "./main.js",
  "type": "module",
  "types": "index.d.ts",
  "version": "7.0.0",
  "keywords": [
    "factory",
    "factory-bot",
    "factory-girl",
    "test",
    "bdd",
    "tdd",
    "fixture",
    "mongodb"
  ],
  "repository": "https://github.com/eflexsystems/factory-bot.git",
  "license": "MIT",
  "files": [
    "*.md",
    "*.txt",
    "*.ts",
    "*.js",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/eflexsystems/factory-bot/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/eflexsystems/factory-bot/raw/master/LICENSE"
    }
  ],
  "dependencies": {
    "chance": "^1.1.10"
  },
  "devDependencies": {
    "bluebird": "^3.7.2",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.34.0",
    "husky": "^4.3.0",
    "istanbul": "^1.0.0-alpha.2",
    "knex": "^0.95.2",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "mocha-eslint": "^7.0.0",
    "mongodb": "^4.14.0",
    "mongoose": "^6.10.0",
    "prettier-eslint-cli": "^5.0.1",
    "redux-orm": "^0.16.2",
    "sequelize": "^6.5.0",
    "should": "^13.2.3",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.5.0",
    "sqlite3": "^5.0.2"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "cross-env NODE_ENV=test mocha",
    "prepublishOnly": "npm test"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended"
    ],
    "env": {
      "mocha": true
    },
    "globals": {
      "Model": "readonly",
      "Promise": "readonly",
      "Set": "readonly",
      "Symbol": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix"
    ]
  },
  "mocha": {
    "recursive": true,
    "exit": true
  }
}
